            tabs = tabsdiv.childNodes[0];
          }else
          {
            tabs = tabsdiv.childNodes[1];
          }
          if(tabs.childNodes.length >= tabIndex)
          {
            tabId = tabs.childNodes[tabIndex-1].id;
            showTab(tabId);        
            location.href = location.href+"#"+ tabId;
          }
        }
      </script></body>
</html>
